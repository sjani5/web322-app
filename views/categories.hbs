<div class="row">
    <div class="col-md-12">
        <h2>Categories</h2>
        <hr />
        <ul>
            {{#each categories}}
            <li><a href="/items?category={{this.categoryId}}">{{this.categoryId}} - {{this.category}}</a></li>
            {{/each}}
        </ul>
    </div>
</div>
